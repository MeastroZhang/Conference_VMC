<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<Window size="400,300" sizebox="4,4,4,4" caption="0,0,0,30" roundcorner="3,3">
	<Font name="微软雅黑" size="14" default="true" />
	<Font id="1" name="微软雅黑" size="12" />
    <VerticalLayout bkcolor="#FFF9F9F9">
		<HorizontalLayout height="30" bkcolor="#FF288ADD">
			<HorizontalLayout />
			<Button name="btnClose" width="30" normalimage="\\common\\btnClose.png" hotimage="\\common\\btnCloseHot.png" />
		</HorizontalLayout>
		<Option name="optRoomVideo" height="20" padding="40,20,0,0" textpadding="30,0,0,0" textcolor="#FF666666" align="left" normalimage="file='\\common\\OptNormal.png' dest='0,0,20,20'" selectedimage="file='\\common\\OptSelect.png' dest='0,0,20,20'"/>
		<Option name="optRoomAudio" height="20" padding="40,10,0,0" textpadding="30,0,0,0" textcolor="#FF666666" align="left" normalimage="file='\\common\\OptNormal.png' dest='0,0,20,20'" selectedimage="file='\\common\\OptSelect.png' dest='0,0,20,20'"/>
        <HorizontalLayout height="20" padding="70,14,40,0">
            <Label name="labRoomPin" height="20" width="110" textcolor="#FF666666" />
			<Edit name="editRoomPin" numberonly="true" height="20" width="180" textpadding="4,3,4,3" bkimage="\\common\\edit180px.png" />
        </HorizontalLayout>
		<HorizontalLayout height="20" padding="70,14,40,0">
            <Label name="labControlPin" height="20" width="110" textcolor="#FF666666" />
			<Edit name="editControlPin" numberonly="true" height="20" width="180" textpadding="4,3,4,3" bkimage="\\common\\edit180px.png" />
        </HorizontalLayout>
		<HorizontalLayout height="20" padding="70,14,40,0">
            <Label name="labRoomLink" height="20" width="110" textcolor="#FF666666" />
			<Edit name="editRoomLink" readonly="true" height="20" width="180" textpadding="4,3,4,3" bkimage="\\common\\edit180px.png" />
        </HorizontalLayout>
		<HorizontalLayout height="20" padding="70,14,40,0">
			<HorizontalLayout />
            <Button name="btnCopy" height="20" width="40" textcolor="#FF288ADD" hottextcolor="#FFFFFFFF" pushedtextcolor="#FF288ADD" normalimage="\\common\\btn64px.png" hotimage="\\common\\btn64pxHot.png" />
			<Button name="btnChange" height="20" width="40" padding="14,0,0,0" textcolor="#FF288ADD" hottextcolor="#FFFFFFFF" pushedtextcolor="#FF288ADD" normalimage="\\common\\btn64px.png" hotimage="\\common\\btn64pxHot.png" />
        </HorizontalLayout>
		<HorizontalLayout height="24" padding="70,14,40,0">
			<HorizontalLayout />
            <Button name="btnCancel" height="24" width="64" textcolor="#FF288ADD" hottextcolor="#FFFFFFFF" pushedtextcolor="#FF288ADD" normalimage="\\common\\btn64px.png" hotimage="\\common\\btn64pxHot.png" />
			<Button name="btnOK" height="24" width="64" padding="20,0,0,0" textcolor="#FF288ADD" hottextcolor="#FFFFFFFF" pushedtextcolor="#FF288ADD" normalimage="\\common\\btn64px.png" hotimage="\\common\\btn64pxHot.png" />
        </HorizontalLayout>
    </VerticalLayout>
</Window>
